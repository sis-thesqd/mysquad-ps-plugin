{
  "id": "com.thesqd.mysquadps.spectrum",
  "name": "mySquad.ps",
  "version": "3.0.0",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "24.4.0"
  },
  "manifestVersion": 5,
  "featureFlags": {
    "enableSWCSupport": true
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "vanilla",
      "label": {
        "default": "mySquad.ps"
      },
      "minimumSize": {
        "width": 280,
        "height": 400
      },
      "maximumSize": {
        "width": 1200,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 350,
        "height": 600
      },
      "preferredFloatingSize": {
        "width": 400,
        "height": 700
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"]
        }
      ]
    }
  ],
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "clipboard": "readAndWrite",
    "launchProcess": {
      "schemes": ["https", "http"],
      "extensions": []
    },
    "webview": {
      "allow": "yes",
      "domains": [
        "https://*.supabase.co",
        "https://*.supabase.io",
        "https://supabase.co",
        "https://supabase.io"
      ]
    },
    "network": {
      "domains": [
        "https://*.supabase.co",
        "https://*.supabase.io",
        "https://supabase.co",
        "https://supabase.io",
        "https://sisx.thesqd.com"
      ]
    }
  },
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark"]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light.png",
      "scale": [1, 2],
      "theme": ["lightest", "light"]
    }
  ]
}
