    "@swc-uxp-wrappers/button": "^2.0.0",
    "@swc-uxp-wrappers/card": "^2.0.0",
    "@swc-uxp-wrappers/menu": "^2.0.0",
    "@swc-uxp-wrappers/overlay": "^2.0.0",
